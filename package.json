{
  "name": "@hudoro/neron",
  "version": "0.1.20",
  "description": "Design System For Web Application",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/esm/index.d.ts",
  "files": [
    "lib/esm/**/*",
    "lib/cjs/**/*"
  ],
  "author": "Prawito Hudoro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hudoro-solusi-digital/neron"
  },
  "bugs": {
    "url": "https://github.com/hudoro-solusi-digital/neron/issues"
  },
  "scripts": {
    "prettier": "prettier --write \"src/**/*.ts\"",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o ../neron-storybook",
    "prepare": "npm run build",
    "patch": "npm version patch --force && npm publish",
    "minor": "npm version minor --force && npm publish",
    "major": "npm version major --force && npm publish"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@storybook/addon-actions": "^6.3.4",
    "@storybook/addon-docs": "^6.3.4",
    "@storybook/addon-essentials": "^6.3.4",
    "@storybook/addon-links": "^6.3.4",
    "@storybook/react": "^6.3.4",
    "@types/node": "^16.3.0",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.11",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.13.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.0",
    "typescript": "^4.3.5"
  }
}
